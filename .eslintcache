ine\n                        transition={{ duration: 0.75 }}\n                        initial={{ width: \"0%\" }}\n                        animate={{\n                            width: pathname === \"/work\" ? \"100%\" : \"0%\",\n                        }}\n                    />\n                </li>\n\n                <li>\n                    <Link to=\"/contact\">3. Contact Us</Link>\n                    <Line\n                        transition={{ duration: 0.75 }}\n                        initial={{ width: \"0%\" }}\n                        animate={{\n                            width: pathname === \"/contact\" ? \"100%\" : \"0%\",\n                        }}\n                    />\n                </li>\n            </ul>\n        </StyledNav>\n    );\n};\n\n// Styles --> function name cannot be the same as styled name --> instead of Nav, use StyledNav or similar\nconst StyledNav = styled.nav`\n    min-height: 10vh;\n    display: flex;\n    margin: auto;\n    justify-content: space-between;\n    align-items: center;\n    padding: 1rem 10rem;\n    background: #282828;\n    position: relative;\n    top: 0;\n    z-index: 10;\n\n    a {\n        color: white;\n        text-decoration: none;\n\n        &::after {\n            width: 100%;\n            background: white;\n        }\n    }\n\n    ul {\n        display: flex;\n        list-style: none;\n    }\n\n    #logo {\n        font-size: 1.5rem;\n        font-family: \"Lobster\", cursive;\n        font-weight: lighter;\n    }\n\n    li {\n        padding-left: 10rem;\n        position: relative;\n    }\n\n    @media (max-width: 1300px) {\n        flex-direction: column;\n        padding: 2rem 1rem 1rem 1rem;\n\n        ul {\n            padding: 2rem;\n            justify-content: space-around;\n            width: 100%;\n        }\n\n        li {\n            padding: 0;\n        }\n\n        #logo {\n            padding: 2rem;\n        }\n    }\n`;\n\nconst Line = styled(motion.div)`\n    height: 0.3rem;\n    background: #23d997;\n    width: 0%;\n    position: relative;\n    top: 30%;\n    left: 0%;\n`;\n\nexport default Nav;\n","/Users/jonathan/Desktop/DevEd/CapturePortfolio/capture/src/pages/OurWork.js",[],"/Users/jonathan/Desktop/DevEd/CapturePortfolio/capture/src/pages/ContactUs.js",[],"/Users/jonathan/Desktop/DevEd/CapturePortfolio/capture/src/pages/MovieDetail.js",["93"],"import React, { useState, useEffect } from \"react\";\nimport styled from \"styled-components\";\nimport { useHistory } from \"react-router-dom\";\nimport { MovieState } from \"../movieState\";\n\n// Animations\nimport { motion } from \"framer-motion\";\nimport { pageAnimation } from \"../animation\";\nimport ScrollTop from \"../components/ScrollTop\";\n\nconst MovieDetail = () => {\n    const history = useHistory();\n    const url = history.location.pathname;\n    const [movies, setMovies] = useState(MovieState);\n    const [movie, setMovie] = useState(null);\n\n    // useEffect\n    useEffect(() => {\n        const currentMovie = movies.filter(\n            (stateMovie) => stateMovie.url === url\n        );\n        setMovie(currentMovie[0]);\n    }, [movies, url]);\n\n    return (\n        <>\n            {movie && (\n                <Details\n                    variants={pageAnimation}\n                    initial=\"hidden\"\n                    animate=\"show\"\n                    exit=\"exit\"\n                >\n                    <HeadLine>\n                        <h2>{movie.title}</h2>\n                        <img src={movie.mainImg} alt=\"Movie\" />\n                    </HeadLine>\n                    <Awards>\n                        {movie.awards.map((award) => (\n                            <Award\n                                title={award.title}\n                                description={award.description}\n                                key={award.title}\n                            />\n                        ))}\n                    </Awards>\n                    <ImageDisplay>\n                        <img src={movie.secondaryImg} alt=\"Movie\" />\n                    </ImageDisplay>\n                </Details>\n            )}\n            <ScrollTop />\n        </>\n    );\n};\n\n// Styles\nconst Details = styled(motion.div)`\n    color: white;\n`;\n\nconst HeadLine = styled.div`\n    min-height: 90vh;\n    padding-top: 20vh;\n    position: relative;\n\n    h2 {\n        position: absolute;\n        top: 10%;\n        left: 50%;\n        transform: translate(-50%, -10%);\n    }\n\n    img {\n        width: 100%;\n        height: 70vh;\n        object-fit: cover;\n    }\n`;\n\nconst Awards = styled.div`\n    min-height: 80vh;\n    display: flex;\n    margin: 5rem 10rem;\n    align-items: center;\n    justify-content: space-around;\n\n    @media (max-width: 1300px) {\n        display: block;\n        margin: 2rem 2rem;\n    }\n`;\n\nconst AwardStyle = styled.div`\n    padding: 5rem;\n\n    h3 {\n        font-size: 2rem;\n    }\n\n    .line {\n        width: 100%;\n        background: #23d997;\n        height: 0.5rem;\n        margin: 1rem 0rem;\n    }\n\n    p {\n        padding: 2rem 0rem;\n    }\n`;\n\nconst ImageDisplay = styled.div`\n    min-height: 50vh;\n\n    img {\n        width: 100%;\n        height: 100vh;\n        object-fit: cover;\n    }\n`;\n\n// Award component\nconst Award = ({ title, description }) => {\n    return (\n        <AwardStyle>\n            <h3>{title}</h3>\n            <div className=\"line\"></div>\n            <p>{description}</p>\n        </AwardStyle>\n    );\n};\n\nexport default MovieDetail;\n","/Users/jonathan/Desktop/DevEd/CapturePortfolio/capture/src/movieState.js",[],"/Users/jonathan/Desktop/DevEd/CapturePortfolio/capture/src/animation.js",[],"/Users/jonathan/Desktop/DevEd/CapturePortfolio/capture/src/components/Wave.js",[],"/Users/jonathan/Desktop/DevEd/CapturePortfolio/capture/src/components/Toggle.js",[],["94","95"],"/Users/jonathan/Desktop/DevEd/CapturePortfolio/capture/src/compone